# auto-grading for an exam result
try:
    score = raw_input("Enter Score: ")
    scr = float(score)
    if scr < 0.6:
        print "F"
    elif scr < 0.7:
        print "D"
    elif scr < 0.8:
        print "C"
    elif scr < 0.9:
        print "B"
    else:
        print "A"
except:
    print "Error, please enter numeric input."
